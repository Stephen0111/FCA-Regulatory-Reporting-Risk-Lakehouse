{
  "datasets": [
    {
      "name": "0cc4b4e5",
      "displayName": "Capital Strength Metrics Analysis",
      "queryLines": [
        "SELECT \n",
        "    capital_adequacy_ratio as value\n",
        "FROM workspace.fca_regulatory.gold_capital_adequacy_ratio"
      ]
    },
    {
      "name": "072fc25a",
      "displayName": "Liquidity Ratio Insights for Analysis",
      "queryLines": [
        "SELECT \n",
        "    liquidity_coverage_ratio as value\n",
        "FROM workspace.fca_regulatory.gold_liquidity_coverage_ratio"
      ]
    },
    {
      "name": "07bfcd8c",
      "displayName": "Suspicious Transactions by Type and Currency Analysis",
      "queryLines": [
        "SELECT \n",
        "    transaction_type,\n",
        "    SUM(CASE WHEN currency = 'GBP' THEN total_amount ELSE 0 END) as GBP,\n",
        "    SUM(CASE WHEN currency = 'USD' THEN total_amount ELSE 0 END) as USD,\n",
        "    SUM(CASE WHEN currency = 'EUR' THEN total_amount ELSE 0 END) as EUR\n",
        "FROM (\n",
        "    SELECT \n",
        "        transaction_type,\n",
        "        currency,\n",
        "        SUM(amount) as total_amount\n",
        "    FROM workspace.fca_regulatory.gold_suspicious_transactions\n",
        "    GROUP BY transaction_type, currency\n",
        ")\n",
        "GROUP BY transaction_type\n",
        "ORDER BY (GBP + USD + EUR) DESC"
      ]
    },
    {
      "name": "3cd4eb4f",
      "displayName": "Top Customer Loan Insights for Analysis",
      "queryLines": [
        "SELECT \n",
        "    customer_id,\n",
        "    COUNT(*) as loan_count,\n",
        "    SUM(amount) as total_loan_amount,\n",
        "    ROUND(AVG(amount), 2) as avg_loan_amount\n",
        "FROM workspace.fca_regulatory.gold_conduct_risk_alerts\n",
        "GROUP BY customer_id\n",
        "ORDER BY total_loan_amount DESC\n",
        "LIMIT 20"
      ]
    },
    {
      "name": "a1f56d88",
      "displayName": "Transaction Summary Insights",
      "queryLines": [
        "SELECT \n",
        "    customer_status,\n",
        "    COUNT(*) as customer_count\n",
        "FROM workspace.fca_regulatory.silver_customer_360\n",
        "GROUP BY customer_status"
      ]
    }
  ],
